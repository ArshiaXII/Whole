<svg width="1200" height="800" viewBox="0 0 1200 800" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="shiitakeBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fefce8;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#fef3c7;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#fde68a;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="shiitakeBrown" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#92400e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#78350f;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="shiitakeAmber" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f59e0b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#d97706;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="shiitakeLight" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#fbbf24;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f59e0b;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="800" fill="url(#shiitakeBg)"/>
  
  <!-- Subtle pattern -->
  <pattern id="shiitakePattern" x="0" y="0" width="120" height="120" patternUnits="userSpaceOnUse">
    <circle cx="60" cy="60" r="2" fill="#92400e" opacity="0.08"/>
    <circle cx="30" cy="90" r="1.5" fill="#f59e0b" opacity="0.06"/>
  </pattern>
  <rect width="1200" height="800" fill="url(#shiitakePattern)"/>
  
  <!-- Main shiitake mushroom cluster illustration -->
  <g transform="translate(600, 400)">
    <!-- Shiitake mushrooms growing from log/substrate -->
    
    <!-- Log/substrate -->
    <ellipse cx="0" cy="90" rx="250" ry="50" fill="url(#shiitakeBrown)" opacity="0.4"/>
    <rect x="-240" y="70" width="480" height="40" fill="url(#shiitakeBrown)" opacity="0.3" rx="20"/>
    
    <!-- Large shiitake mushroom (center) -->
    <g transform="translate(0, 0)">
      <!-- Thick stem -->
      <ellipse cx="0" cy="50" rx="12" ry="35" fill="url(#shiitakeLight)"/>
      <!-- Round brown cap with white spots -->
      <circle cx="0" cy="15" r="35" fill="url(#shiitakeBrown)"/>
      <!-- White spots on cap -->
      <circle cx="-15" cy="5" r="3" fill="white" opacity="0.8"/>
      <circle cx="10" cy="-5" r="2.5" fill="white" opacity="0.8"/>
      <circle cx="-8" cy="20" r="2" fill="white" opacity="0.8"/>
      <circle cx="18" cy="12" r="2.5" fill="white" opacity="0.8"/>
      <circle cx="-25" cy="-8" r="1.5" fill="white" opacity="0.8"/>
      <circle cx="5" cy="25" r="1.5" fill="white" opacity="0.8"/>
      <!-- Gills underneath -->
      <g opacity="0.7">
        <path d="M-30,20 Q-15,35 0,38 Q15,35 30,20" stroke="url(#shiitakeAmber)" stroke-width="1.5" fill="none"/>
        <path d="M-25,22 Q-10,36 0,39 Q10,36 25,22" stroke="url(#shiitakeAmber)" stroke-width="1.5" fill="none"/>
        <path d="M-20,24 Q-5,37 0,40 Q5,37 20,24" stroke="url(#shiitakeAmber)" stroke-width="1.5" fill="none"/>
        <path d="M-15,26 Q0,38 0,41 Q0,38 15,26" stroke="url(#shiitakeAmber)" stroke-width="1.5" fill="none"/>
      </g>
    </g>
    
    <!-- Medium shiitake mushroom (left) -->
    <g transform="translate(-100, 20)">
      <!-- Stem -->
      <ellipse cx="0" cy="40" rx="10" ry="28" fill="url(#shiitakeLight)"/>
      <!-- Round cap -->
      <circle cx="0" cy="12" r="28" fill="url(#shiitakeBrown)"/>
      <!-- White spots -->
      <circle cx="-12" cy="3" r="2.5" fill="white" opacity="0.8"/>
      <circle cx="8" cy="-3" r="2" fill="white" opacity="0.8"/>
      <circle cx="-6" cy="18" r="1.5" fill="white" opacity="0.8"/>
      <circle cx="15" cy="10" r="2" fill="white" opacity="0.8"/>
      <circle cx="-20" cy="-6" r="1" fill="white" opacity="0.8"/>
      <!-- Gills -->
      <g opacity="0.7">
        <path d="M-24,16 Q-12,28 0,30 Q12,28 24,16" stroke="url(#shiitakeAmber)" stroke-width="1.2" fill="none"/>
        <path d="M-20,18 Q-8,29 0,31 Q8,29 20,18" stroke="url(#shiitakeAmber)" stroke-width="1.2" fill="none"/>
      </g>
    </g>
    
    <!-- Medium shiitake mushroom (right) -->
    <g transform="translate(110, 25)">
      <!-- Stem -->
      <ellipse cx="0" cy="42" rx="9" ry="30" fill="url(#shiitakeLight)"/>
      <!-- Round cap -->
      <circle cx="0" cy="12" r="30" fill="url(#shiitakeBrown)"/>
      <!-- White spots -->
      <circle cx="-10" cy="5" r="2" fill="white" opacity="0.8"/>
      <circle cx="12" cy="-8" r="2.5" fill="white" opacity="0.8"/>
      <circle cx="-18" cy="15" r="1.5" fill="white" opacity="0.8"/>
      <circle cx="6" cy="20" r="1.5" fill="white" opacity="0.8"/>
      <circle cx="22" cy="8" r="2" fill="white" opacity="0.8"/>
      <!-- Gills -->
      <g opacity="0.7">
        <path d="M-26,18 Q-13,30 0,32 Q13,30 26,18" stroke="url(#shiitakeAmber)" stroke-width="1.2" fill="none"/>
        <path d="M-22,20 Q-9,31 0,33 Q9,31 22,20" stroke="url(#shiitakeAmber)" stroke-width="1.2" fill="none"/>
      </g>
    </g>
    
    <!-- Small shiitake mushroom (back left) -->
    <g transform="translate(-180, -10)">
      <!-- Stem -->
      <ellipse cx="0" cy="30" rx="7" ry="20" fill="url(#shiitakeLight)"/>
      <!-- Round cap -->
      <circle cx="0" cy="10" r="20" fill="url(#shiitakeBrown)"/>
      <!-- White spots -->
      <circle cx="-8" cy="2" r="1.5" fill="white" opacity="0.8"/>
      <circle cx="6" cy="-5" r="1" fill="white" opacity="0.8"/>
      <circle cx="-4" cy="15" r="1" fill="white" opacity="0.8"/>
      <circle cx="12" cy="8" r="1.5" fill="white" opacity="0.8"/>
      <!-- Gills -->
      <g opacity="0.7">
        <path d="M-17,14 Q-8,22 0,24 Q8,22 17,14" stroke="url(#shiitakeAmber)" stroke-width="1" fill="none"/>
      </g>
    </g>
    
    <!-- Small shiitake mushroom (back right) -->
    <g transform="translate(190, -5)">
      <!-- Stem -->
      <ellipse cx="0" cy="32" rx="8" ry="22" fill="url(#shiitakeLight)"/>
      <!-- Round cap -->
      <circle cx="0" cy="10" r="22" fill="url(#shiitakeBrown)"/>
      <!-- White spots -->
      <circle cx="-9" cy="3" r="1.5" fill="white" opacity="0.8"/>
      <circle cx="7" cy="-6" r="1.5" fill="white" opacity="0.8"/>
      <circle cx="-5" cy="16" r="1" fill="white" opacity="0.8"/>
      <circle cx="14" cy="9" r="1.5" fill="white" opacity="0.8"/>
      <!-- Gills -->
      <g opacity="0.7">
        <path d="M-19,15 Q-9,24 0,26 Q9,24 19,15" stroke="url(#shiitakeAmber)" stroke-width="1" fill="none"/>
      </g>
    </g>
    
    <!-- Tiny shiitake mushrooms -->
    <g transform="translate(-220, 45)">
      <ellipse cx="0" cy="20" rx="5" ry="15" fill="url(#shiitakeLight)"/>
      <circle cx="0" cy="5" r="12" fill="url(#shiitakeBrown)"/>
      <circle cx="-4" cy="2" r="1" fill="white" opacity="0.8"/>
      <circle cx="3" cy="-2" r="0.8" fill="white" opacity="0.8"/>
    </g>
    
    <g transform="translate(230, 50)">
      <ellipse cx="0" cy="22" rx="6" ry="16" fill="url(#shiitakeLight)"/>
      <circle cx="0" cy="6" r="14" fill="url(#shiitakeBrown)"/>
      <circle cx="-5" cy="3" r="1" fill="white" opacity="0.8"/>
      <circle cx="4" cy="-3" r="1" fill="white" opacity="0.8"/>
    </g>
  </g>
  
  <!-- Soil/growing medium base -->
  <rect x="0" y="650" width="1200" height="150" fill="#8b5cf6" opacity="0.1"/>
  
  <!-- Product label -->
  <rect x="50" y="50" width="280" height="120" fill="white" rx="15" opacity="0.95"/>
  <text x="190" y="85" text-anchor="middle" fill="#92400e" font-family="Arial, sans-serif" font-size="24" font-weight="bold">
    Shiitake
  </text>
  <text x="190" y="110" text-anchor="middle" fill="#f59e0b" font-family="Arial, sans-serif" font-size="16">
    Rich &amp; Umami
  </text>
  <text x="190" y="135" text-anchor="middle" fill="#6b7280" font-family="Arial, sans-serif" font-size="12" style="font-style: italic;">
    Lentinula edodes
  </text>
  
  <!-- Decorative elements -->
  <circle cx="1050" cy="150" r="8" fill="#92400e" opacity="0.3"/>
  <circle cx="1100" cy="200" r="6" fill="#f59e0b" opacity="0.4"/>
  <circle cx="150" cy="650" r="10" fill="#78350f" opacity="0.3"/>
  <circle cx="100" cy="700" r="7" fill="#d97706" opacity="0.4"/>
</svg>
