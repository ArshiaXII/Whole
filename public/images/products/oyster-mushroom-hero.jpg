<svg width="1200" height="800" viewBox="0 0 1200 800" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background gradient -->
  <defs>
    <linearGradient id="oysterBg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8fafc;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#f1f5f9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e2e8f0;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="oysterGray" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#64748b;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#475569;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="oysterLight" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f1f5f9;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#cbd5e1;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="oysterDark" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#334155;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1e293b;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="1200" height="800" fill="url(#oysterBg)"/>
  
  <!-- Subtle pattern -->
  <pattern id="oysterPattern" x="0" y="0" width="100" height="100" patternUnits="userSpaceOnUse">
    <circle cx="50" cy="50" r="1.5" fill="#64748b" opacity="0.08"/>
    <circle cx="25" cy="75" r="1" fill="#475569" opacity="0.06"/>
  </pattern>
  <rect width="1200" height="800" fill="url(#oysterPattern)"/>
  
  <!-- Main oyster mushroom cluster illustration -->
  <g transform="translate(600, 400)">
    <!-- Oyster mushroom cluster growing from substrate -->
    
    <!-- Substrate/growing medium -->
    <ellipse cx="0" cy="80" rx="200" ry="40" fill="url(#oysterDark)" opacity="0.3"/>
    
    <!-- Large oyster mushroom (center) -->
    <g transform="translate(0, 0)">
      <!-- Stem -->
      <ellipse cx="0" cy="40" rx="8" ry="25" fill="url(#oysterLight)"/>
      <!-- Fan-shaped cap -->
      <path d="M-40,20 Q-50,0 -30,-10 Q-10,-15 10,-10 Q30,-5 40,10 Q35,25 20,30 Q0,35 -20,30 Q-35,25 -40,20 Z" fill="url(#oysterGray)"/>
      <!-- Gills underneath -->
      <g opacity="0.6">
        <path d="M-35,22 Q-20,28 0,30 Q20,28 35,22" stroke="url(#oysterDark)" stroke-width="1" fill="none"/>
        <path d="M-30,24 Q-15,29 0,31 Q15,29 30,24" stroke="url(#oysterDark)" stroke-width="1" fill="none"/>
        <path d="M-25,26 Q-10,30 0,32 Q10,30 25,26" stroke="url(#oysterDark)" stroke-width="1" fill="none"/>
      </g>
    </g>
    
    <!-- Medium oyster mushroom (left) -->
    <g transform="translate(-80, 20)">
      <!-- Stem -->
      <ellipse cx="0" cy="35" rx="6" ry="20" fill="url(#oysterLight)"/>
      <!-- Fan-shaped cap -->
      <path d="M-30,15 Q-38,-2 -22,-8 Q-8,-12 8,-8 Q22,-4 30,8 Q26,20 15,24 Q0,27 -15,24 Q-26,20 -30,15 Z" fill="url(#oysterGray)"/>
      <!-- Gills -->
      <g opacity="0.6">
        <path d="M-26,17 Q-12,22 0,24 Q12,22 26,17" stroke="url(#oysterDark)" stroke-width="0.8" fill="none"/>
        <path d="M-22,19 Q-8,23 0,25 Q8,23 22,19" stroke="url(#oysterDark)" stroke-width="0.8" fill="none"/>
      </g>
    </g>
    
    <!-- Medium oyster mushroom (right) -->
    <g transform="translate(90, 15)">
      <!-- Stem -->
      <ellipse cx="0" cy="38" rx="7" ry="22" fill="url(#oysterLight)"/>
      <!-- Fan-shaped cap -->
      <path d="M-35,18 Q-42,0 -25,-6 Q-10,-10 10,-6 Q25,-2 32,12 Q28,24 18,28 Q0,31 -18,28 Q-30,24 -35,18 Z" fill="url(#oysterGray)"/>
      <!-- Gills -->
      <g opacity="0.6">
        <path d="M-30,20 Q-15,25 0,27 Q15,25 28,20" stroke="url(#oysterDark)" stroke-width="0.8" fill="none"/>
        <path d="M-25,22 Q-10,26 0,28 Q10,26 25,22" stroke="url(#oysterDark)" stroke-width="0.8" fill="none"/>
      </g>
    </g>
    
    <!-- Small oyster mushroom (top left) -->
    <g transform="translate(-120, -20)">
      <!-- Stem -->
      <ellipse cx="0" cy="25" rx="4" ry="15" fill="url(#oysterLight)"/>
      <!-- Fan-shaped cap -->
      <path d="M-20,10 Q-25,-5 -15,-8 Q-5,-10 5,-8 Q15,-5 20,5 Q17,15 10,18 Q0,20 -10,18 Q-17,15 -20,10 Z" fill="url(#oysterGray)"/>
      <!-- Gills -->
      <g opacity="0.6">
        <path d="M-17,12 Q-8,16 0,17 Q8,16 17,12" stroke="url(#oysterDark)" stroke-width="0.6" fill="none"/>
      </g>
    </g>
    
    <!-- Small oyster mushroom (top right) -->
    <g transform="translate(130, -15)">
      <!-- Stem -->
      <ellipse cx="0" cy="28" rx="5" ry="18" fill="url(#oysterLight)"/>
      <!-- Fan-shaped cap -->
      <path d="M-22,12 Q-28,-3 -18,-6 Q-8,-8 8,-6 Q18,-3 22,8 Q19,18 12,21 Q0,23 -12,21 Q-19,18 -22,12 Z" fill="url(#oysterGray)"/>
      <!-- Gills -->
      <g opacity="0.6">
        <path d="M-19,14 Q-9,18 0,19 Q9,18 19,14" stroke="url(#oysterDark)" stroke-width="0.6" fill="none"/>
      </g>
    </g>
    
    <!-- Tiny oyster mushrooms -->
    <g transform="translate(-160, 40)">
      <ellipse cx="0" cy="15" rx="3" ry="10" fill="url(#oysterLight)"/>
      <path d="M-12,8 Q-15,-2 -8,-4 Q-3,-5 3,-4 Q8,-2 12,3 Q10,10 6,12 Q0,13 -6,12 Q-10,10 -12,8 Z" fill="url(#oysterGray)"/>
    </g>
    
    <g transform="translate(170, 35)">
      <ellipse cx="0" cy="18" rx="3" ry="12" fill="url(#oysterLight)"/>
      <path d="M-14,10 Q-17,-1 -10,-3 Q-4,-4 4,-3 Q10,-1 14,5 Q12,12 8,14 Q0,15 -8,14 Q-12,12 -14,10 Z" fill="url(#oysterGray)"/>
    </g>
  </g>
  
  <!-- Soil/growing medium base -->
  <rect x="0" y="650" width="1200" height="150" fill="#8b5cf6" opacity="0.1"/>
  
  <!-- Product label -->
  <rect x="50" y="50" width="320" height="120" fill="white" rx="15" opacity="0.95"/>
  <text x="210" y="85" text-anchor="middle" fill="#64748b" font-family="Arial, sans-serif" font-size="24" font-weight="bold">
    Oyster Mushroom
  </text>
  <text x="210" y="110" text-anchor="middle" fill="#475569" font-family="Arial, sans-serif" font-size="16">
    Delicate &amp; Savory
  </text>
  <text x="210" y="135" text-anchor="middle" fill="#6b7280" font-family="Arial, sans-serif" font-size="12" style="font-style: italic;">
    Pleurotus ostreatus
  </text>
  
  <!-- Decorative elements -->
  <circle cx="1050" cy="150" r="8" fill="#64748b" opacity="0.3"/>
  <circle cx="1100" cy="200" r="6" fill="#475569" opacity="0.4"/>
  <circle cx="150" cy="650" r="10" fill="#334155" opacity="0.3"/>
  <circle cx="100" cy="700" r="7" fill="#1e293b" opacity="0.4"/>
</svg>
